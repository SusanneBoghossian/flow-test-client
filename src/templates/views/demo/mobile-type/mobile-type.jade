extends ../../../layouts/demo

block content
	.container
		.well
			.row
				h4 Ihr Mobile Status.
			form.row
				.form-group
					label= status.tel
				.form-group
					label Active IMSI
					input.form-control(name="activeIMSI", value= status.imsi, type="text", readonly)
				.form-group
					label Call Forwarding
					input.form-control(name="callForwarding", value= status.roaming, type="text", readonly)
				.form-group
					label Billing Type
					input.form-control(name="billingType", value= status.billingType, type="text", readonly)
				.form-group
					a.btn.btn-default(href="./") Cancel